<div v-view-setup="PPrdMarketCtrl" v-init="init()">
	<!-- 超市首页面 -->
	<div v-page v-transition="native" title="理财超市">
		<div class="showborder">
			<div class=" lic_buy"  v-show="lijin.TplCd=='1102'" v-repeat="lijin in ProdectList" v-click="goLijinInfo(lijin)">
				<h3 style="padding-bottom:5px;">
					<span style="font-weight: bold; font-size: 14px;width:55%;overflow:hidden;">{{lijin.PrdctNm}}</span>
<!-- 					<span class="licaiTitle">{{lijin.prdType|LJPrdTypeFilter}}</span> -->
					<div class="PrdOutsiteBox" style="border-radius:5px;float:right;">
						可售额度{{(lijin.AvlMblLmt*100+lijin.AvlPrsnLmt*100)/lijin.PrdctMaxRsgAmt|number:0}}%
						<div class="PrdInnerBox" style="border-radius:5px;width:{{((lijin.AvlMblLmt*100+lijin.AvlPrsnLmt*100)/lijin.PrdctMaxRsgAmt|number:0)==1?0:(lijin.AvlMblLmt*100+lijin.AvlPrsnLmt*100)/lijin.PrdctMaxRsgAmt|number:0}}%;margin-left:{{(lijin.PrdctMaxRsgAmt*100-(lijin.AvlMblLmt*100+lijin.AvlPrsnLmt*100))/lijin.PrdctMaxRsgAmt|number:0}}%"></div>
					</div>
				</h3>
				<div class="xiangq_img1"><a href=""><img src="images/more.png" /></a></div>
				<div class="earn licaiInfo" style="margin-top:10px;">
					<span class="superwidth">
						<span style="font-size:20px;color:#f18903;text-align:center;" >{{lijin.ExpcPftRateTxt}}</span>
						&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;预期年化收益<br />
					</span>
<!-- 					<span v-show="lijin.ExpcPftRateTxt==lijin.expectMaxRate$$lijin.expectMaxRate==0.0000" class="superwidth"> -->
<!-- 						<span style="font-size:20px;color:#f18903;padding-left:30px;" >{{lijin.expIncomeRate|FloatTwo}}%</span> -->
<!-- 						&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;预期年化收益<br /> -->
<!-- 					</span> -->
					<span>
						<span style="color:#0084ff">{{lijin.PrsnFrstMinIvsAmt/10000||"--"}}万</span>
						起购金额<br />
					</span>
					<span>
						<span style="color:#666">{{lijin.CycDays}}天</span>
						产品期限<br />
					</span>
<!-- 					<div class="PrdOutsiteBox" style="border-radius:5px;"> -->
<!-- 						可售额度{{(lijin.AvlMblLmt+lijin.AvlPrsnLmt)*100/lijin.PrdctMaxRsgAmt|number:0}}% -->
<!-- 						<div class="PrdInnerBox" style="border-radius:5px;width:{{((lijin.AvlMblLmt+lijin.AvlPrsnLmt)*100/lijin.PrdctMaxRsgAmt|number:0)==1?0:(lijin.AvlMblLmt+lijin.AvlPrsnLmt)*100/lijin.PrdctMaxRsgAmt|number:0}}%;margin-left:{{(lijin.PrdctMaxRsgAmt-(lijin.AvlMblLmt+lijin.AvlPrsnLmt))*100/lijin.PrdctMaxRsgAmt|number:0}}%"></div> -->
<!-- 					</div> -->
					<!-- <span style="overflow:hidden;width:auto; float:right;text-align:right;">
						{{lijin.IpoStartDate}}-{{lijin.ipoEndDate}}
					</span> -->
				</div>
			</div>

			<div class="lic_buy" v-repeat="tt in ttProdects"  v-click="goTTInfo(tt)">
				<h3 style="padding-bottom:5px;">
					<span style="font-weight: bold; font-size: 14px;width:100%;overflow:hidden;">{{tt.prdName}}</span>
				</h3>
				<div class="xiangq_img1"><a href=""><img src="images/more.png" /></a></div>
				<div class="earn licaiInfo" style="margin-top:10px;" >
					<span class="superwidth" style="text-align:left;">
						<span style="color:#f18903;font-size:20px;padding-left:30px;">{{tt.rtnRate|FloatTwo}}%</span>
						&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;预期年化收益<br />
					</span>
					<span style="text-align:center;width:40%">
						<span style="color:#0084ff;">{{tt.firstBuyAmt/10000}}万</span>
						起购金额<br />
					</span>
				</div>
			</div>

		</div>
		<div class="zz_cont"><img src="images/ts.png" />
		    <font class="tishi">
				温馨提示</br>
			</font>
				&nbsp;&nbsp;1、夏令时（5月1日至9月30日），产品募集期最后一日16:30以后不能再进行产品认购交易。<br/>
				&nbsp;&nbsp;2、冬令时（10月1日至4月30日），产品募集期最后一日16:00以后不能再进行产品认购交易。
			<div style="font-size:12px; line-height:16px; color:#929292;"> </div>
	   </div>
		<div style="width:100%;height:40px"></div>
	</div>
</div>
<script>
PPrdMarketCtrl.$inject = ["$scope","$remote","$targets","$filter","$context"];
function PPrdMarketCtrl($scope,$remote,$targets,$filter,$context){
	$scope.init=function(){
		var params={
				"state":1,
				"pageSize":500,
				"currentIndex":1
		}
		//粒金理财产品查询(列表)
		$remote.post("/pmobile/QueryLJLCProductList.do",{"pageSize":500,"currentIndex":1,"PrdctSt":"1"},function(data){
// 			var temp=[];
// 			vx.forEach(data.List,function(item){
// 			if(item.state==1){//0:开放期,  1:募集期,  2:发行成功,a:产品终止
// 				temp.push(item);
// 			}
// 			});
// 			console.log(data);
			$scope.ProdectList=data.List;
		});
		var params={
			"pageSize":500,
			"currentIndex":1
		}
		//天天利产品查询
		$remote.post("/pmobile/QueryTTLProductInfo.do",params,function(data){
			$scope.ttProdects=data.List;
			$scope.TTCurrencyType="01";
		});
	}
	//跳转粒金理财详情页
	$scope.goLijinInfo=function(obj){
		//$scope.selectedLJ.lastDate=$scope.formartDate(obj.endDate,1);
// 		$context.setJson({"selectedLiCai":obj.prdCode,"selecType":"LiJin"});
// 		alert(obj.AvlMblLmt+"============AvlMblLmt");
// 		alert(obj.AvlPrsnLmt+"------------AvlPrsnLmt");
// 		alert(obj.PrdctMaxRsgAmt+"++++++++++++++PrdctMaxRsgAmt");

		$context.setJson({"selectedLiCai":obj.PrdctCd,"selecType":"LiJin","prdInfo":obj});
		$scope.goto("PPrdMarket","PPrdDetailInfo");
	}
	//跳转天天理财详情页
	$scope.goTTInfo=function(obj){
		console.log(obj);
		$context.setJson({"selectedLiCai":obj.prdCode,"selecType":"TTLi"});
		$scope.goto("PPrdMarket","PPrdDetailInfo");
	}
}
</script>