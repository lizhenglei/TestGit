<script type="text/javascript">
	YaoYiYaoCtrl.$inject = [ "$scope", "$targets", "$remote", "Util", "$http",
			"$rootScope", '$timeout', "$os", "$context" ];
	function YaoYiYaoCtrl($scope, $targets, $remote, Util, $http, $rootScope,
			$timeout, $os, $context) {
		$scope.init = function() {
			$scope.jiantou = document.getElementById("jiantou");
			$scope.mydiv = document.getElementById("mydiv");
			$scope.initFlagJN = false;
			$scope.GameRule();
			$("#floatId").hide();
			$("body #page1").css({
				"background-image" : "url('images/YaoYiYao04.png')",
				"background-size" : "cover",
				"width" : "100%",
				"min-height" : "100%"
			});
			$scope.o = 0;
			$scope.i = 0;
			$scope.timer1 = null;
			$scope.zongshu = 0;
			$scope.IsReward = 0;
			$scope.k = 0;
			$scope.wtf = 0;
			$scope.wtf = $context.getJson().innerParam;
			if (!$scope.wtf) {
				$scope.k = 1;
			} else {
				$scope.IsReward = $scope.wtf.IsReward;
			}
			$remote.post("/pmobile/TransRewardsRuleQry.do", {
				"gameType" : "3"
			}, function(data) {
				$scope.activityRule = data.activityRule;
				$scope.activityRule = $scope.activityRule.replace(/br/g,
						"</br>");
			})
			$scope.getClientState(function(response) {
				var resJson = vx.fromJson(response);
				$scope.userType = resJson.Userinfo.CifNo;
				$scope.HexinKehuhao = resJson.Userinfo.HexinKehuhao;
			});
			$scope.JoinActivity();
			
		};

		// 游戏规则
		$scope.GameRule = function() {
			$scope.showContent1 = true;
			$scope.showContent2 = false;
			$scope.showContent3 = false;
			$("#tab1").attr("class", "tab11");
			$("#tab2").attr("class", "tab2");
			$("#tab3").attr("class", "tab3");
			$("#tab").css({
				"background-color" : "rgb(255, 196, 20)"
			});
		};
		// 中奖信息
		$scope.WinningInfo = function() {
			$scope.showContent1 = false;
			$scope.showContent2 = true;
			$scope.showContent3 = false;
			$("#tab1").attr("class", "tab1");
			$("#tab2").attr("class", "tab22");
			$("#tab3").attr("class", "tab3");
			$("#tab").css({
				"background-color" : "rgb(255, 196, 20)"
			});
			$remote.post("/pweb/QryPrizeInfos.do", {}, function(data) {
				$scope.PrizeList = data.List;
			});
		};
		// 免责声明
		$scope.Disclaimer = function() {
			$scope.showContent1 = false;
			$scope.showContent2 = false;
			$scope.showContent3 = true;
			$("#tab1").attr("class", "tab1");
			$("#tab2").attr("class", "tab2");
			$("#tab3").attr("class", "tab33");
			$("#tab").css({
				"background-color" : "rgb(250, 233, 187)"
			});
		};

		// 立即参与摇奖活动
		$scope.JoinActivity = function() {
			if ($scope.k == 1) {
				// 				if (null == $scope.MobilePhone || "" == $scope.MobilePhone) {
				// 					alert("手机号码不能为空！");
				// 					return;
				// 				}
				// 				if (!(/^1[3|4|5|8][0-9]\d{4,8}$/.test($scope.MobilePhone))) {
				// 					alert("手机号码样式不正确！请重新输入");
				// 					return;
				// 				}
				// 查询用户的抽奖次数
				$scope.getClientState(function(response) {
					var resJson=vx.fromJson(response);
					$scope.mobileNo=resJson.Userinfo.MobileNo;
					var params = {
						"MobilePhone" : $scope.MobilePhone
					}
					$context.setJson({
						"phone" : params
					});
					$scope.ph = $context.getJson().phone;
					NativeCall.hideMask();
					NativeCall.ShakeMobile($scope.yaojiang);
				});
			} else {
				NativeCall.hideMask();
				NativeCall.ShakeMobile($scope.yaojiang);
			}
		};

		// 摇奖实现函数
		$scope.yaojiang = function() {
			if ($scope.k == 0) {
				if ($scope.o == 0) {
					$scope.o++;
					$remote.post("/pmobile/getNewTokenName.do", {}, function(
							data) {
						$scope.tokenName = data._tokenName;
						params = {
							"TRANSNAME" : $scope.wtf.TransName,
							"_tokenName" : $scope.tokenName,
							"clientNo" : $scope.userType,
							"hostCustNo" : $scope.HexinKehuhao
						}
						$remote.post("/pmobile/PlayTransRewards.do", params,
								function(data) {
									$scope.f1 = data.PrizeGrade;
									$scope.f2 = data.PrizeName;
									if ($scope.f1 == "1") {
										$scope.nam = "一";
									}
									if ($scope.f1 == "2") {
										$scope.nam = "二";
									}
									if ($scope.f1 == "3") {
										$scope.nam = "三";
									}
									if (data.IsLucky == 0) {
										// 										alert("恭喜您已中" + $scope.f1
										// 												+ "奖！奖品为：" + $scope.f2);
										var param = {
											"message" : "恭喜你获得" + $scope.nam
													+ "等奖！\n 奖品为：" + $scope.f2
										}
										$scope.alertK(function() {
										}, "恭喜你获得" + $scope.nam + "等奖！\n 奖品为："
												+ $scope.f2);
									} else if (data.IsLucky == 1) {
										// 										alert("还差一点，请继续努力！");
										var param = {
											"message" : "还差一点，请继续努力！"
										}
										$scope.alertK(function() {
										}, "还差一点，请继续努力！");
									}
								}, function(data) {
									var param = {
										"message" : data.jsonError
									}
									$scope.alertK(function() {
									}, vx.toJson(param));
									return false;
								});
					});

				} else {
					// 					alert("对不起 你的抽奖次数已用没");
					var param = {
						"message" : "对不起 你的抽奖次数已用没"
					}
					$scope.alertK(function() {
					}, "对不起 你的抽奖次数已用没");
					return false;
				}
			} else {
				$remote.post("/pmobile/getNewTokenName.do", {}, function(data) {
					$scope.ph = $context.getJson().phone;
					$scope.tokenName = data._tokenName;
					params = {
						"TRANSNAME" : "YaoYiYao",
						"_tokenName" : $scope.tokenName,
						"PHONE" : $scope.ph.MobilePhone,
						"clientNo" : $scope.userType,
						"hostCustNo" : $scope.HexinKehuhao
					}
					$remote.post("/pmobile/PlayTransRewards.do", params,
							function(data) {
								$scope.f1 = data.PrizeGrade;
								$scope.f2 = data.PrizeName;
								if ($scope.f1 == "1") {
									$scope.nam = "一";
								}
								if ($scope.f1 == "2") {
									$scope.nam = "二";
								}
								if ($scope.f1 == "3") {
									$scope.nam = "三";
								}
								if (data.IsLucky == 0) {
									var param = {
										"message" : "恭喜你获得" + $scope.nam
												+ "等奖！\n 奖品为：" + $scope.f2
									}
									$scope.alertK(function() {
									}, "恭喜你获得" + $scope.nam + "等奖！\n 奖品为："
											+ $scope.f2);
								} else if (data.IsLucky == 1) {
									var param = {
										"message" : "还差一点，请继续努力！"
									}
									$scope.alertK(function() {
									}, "还差一点，请继续努力！");
								}
							}, function(data) {
								var param = {
									"message" : data.jsonError
								}
								$scope.alertK(function() {
								}, vx.toJson(param));
								return false;
							});
				});
			}

			// 		$("#wallet1Shake,#wallet2Shake, #wallet3Shake, #wallet6Shake,#wallet9Shake,#wallet11Shake").removeClass().addClass("shake");
			// 		$("#wallet4Shake,#wallet5Shake,#wallet7Shake,#wallet8Shake,#wallet10Shake,#wallet12Shake,#wallet13Shake").removeClass().addClass("shake1");
			// 		$remote.post("/pmobile/getNewTokenName.do",{},function(data1){
			// 			$remote.post("/pmobile/PlayTransRewards.do", {"_tokenName" : data1._tokenName,"MobilePhone" : $scope.MobilePhone,"ClientType" : 3,"GameType" : 3},function(data) {
			// 				if(data._RejCode=="000000"){
			// 					if(data.IsLucky==0){
			// 						$scope.alert("恭喜您已中奖！奖品为："+data.PrizeName);
			// 					}else if(data.IsLucky==1){
			// 						$scope.alert("还差一点，请继续努力！");
			// 					}
			// 					$scope.PlayTimes=$scope.PlayTimes-1;
			// 					NativeCall.ShakeMobile($scope.yaojiang);
			// 				}else{
			// 					$scope.alert(data.jsonError);
			// 					NativeCall.ShakeMobile($scope.yaojiang);
			// 				}

			// 			});
			// 		});
		};

		// 分享给好友:传参数到原生处理
		$scope.JoinToFriend = function() {
			var formData0 = {
				"Picture" : "",
				"MeAddr" : "",
				"MeifType" : "",
				"MeName" : "常熟农商银行摇一摇抽奖活动",
				"MeinTroduce" : "摇一摇，赢大奖啦"
			};
			var formData1 = {
				"type" : "shake",
				"Message" : formData0,
				// 	        "ShareUrl":"http://32.114.153.239:8088/pweb/checkApp.do?PageName=YaoYiYao"
				// 			 "ShareUrl":"http://32.114.62.202:9081/pweb/checkApp.do?PageName=YaoYiYao"
				"ShareUrl" : "https://mbl.jnbank.cc/pweb/checkApp.do?PageName=YaoYiYao"
			};
			NativeCall.ShareMessage(vx.toJson(formData1));
		};
	};
</script>
