	word-spacing: -2px;
	width: 220px;
	height: 250px;
	padding: 80px 37px 100px 22px;
	text-align: justify;
}

#close {
	background: no-repeat;
	webkit-border-radius: 15px;
	border-radius: 15px;
	width: 30px;
	height: 30px;
	padding-left: 2px;
	padding-right: 2px;
	color: rgb(75, 74, 74);
	cursor: pointer;
	position: absolute;
	right: 0;
	top: 5px;
	font-size: 8px;
	z-index: 2000000001 !important;
}

#joinfrend {
	background: no-repeat;
	webkit-border-radius: 4px;
	cursor: pointer;
	position: absolute;
	width: 141px;
	height: 30px;
	bottom: 37px;
	left: 33px;
	text-align: center;
	font-size: 14px;
	z-index: 2000000001 !important;
}
