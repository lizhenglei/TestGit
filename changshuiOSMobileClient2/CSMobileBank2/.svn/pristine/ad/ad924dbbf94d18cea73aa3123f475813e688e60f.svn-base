	$scope.toSignConfirm=function(){
		//初始化确认页面
		$scope.keypassword="";
		
		if($scope.SingLimit == undefined||$scope.SingLimit==null || $scope.SingLimit==""){
			NativeCall.toast("请输入单笔支付限额");
			return;
		}			
		if($scope.DayLimit == undefined||$scope.DayLimit==null || $scope.DayLimit==""){
			NativeCall.toast("请输入日累计支付限额");
			return;
		}	
		if(parseInt($scope.SingLimit)>parseInt($scope.DayLimit)){
			NativeCall.toast("单笔限额不得大于日累计限额");
			return;
		}	
		
		if($scope.agree != true){
			NativeCall.toast("请认真阅读并同意《个人支付签约协议》");
			return;
		}		
		$scope.getNewToken();
		$targets("content", "#+1");
	} 
	
