</div>
<script>
PPrdMarketCtrl.$inject = ["$scope","$remote","$targets","$filter","$context"];
function PPrdMarketCtrl($scope,$remote,$targets,$filter,$context){
	$scope.init=function(){
// 		var params={
// 				"state":1,
// 				"pageSize":10,
// 				"currentIndex":1
// 		}
		//粒金理财产品查询(列表)
		$remote.post("/pmobile/QueryLJLCProductList.do",{"pageSize":10,"currentIndex":1,"PrdctSt":"1,0"},function(data){
			$scope.ProdectList=data.List;
			$scope.totalnumber = data.TotalNum;
		});
		var params={
			"pageSize":500,
			"currentIndex":1
		}
	}
	$scope.morepage = function(qishi,zhongzhi,zengliang,list){
		$scope.start=1;//不是第一次进入
		var params = {
				"PrdctSt":"1,0",
				"pageSize":10,
				"currentIndex":qishi/10+1
		};
		$remote.post("/pmobile/QueryLJLCProductList.do", params, function(data) {
			$scope.ProdectList=data.List;
		});
	};
	//跳转粒金理财详情页
	$scope.goLijinInfo=function(obj){
		$context.setJson({"selectedLiCai":obj.PrdctCd,"selecType":"LiJin","prdInfo":obj});
		$scope.goto("PPrdMarket","PPrdDetailInfo");
	}
}
</script>