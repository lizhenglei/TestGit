				   	  <ul>
			            	<li><em style="width: 40%;" >理财产品：</em><i style="width: 60%;">{{ljInfo.PrdctNm}}</i></li>
							<li><em style="width: 40%;" >购买金额：</em><i style="width: 60%;">{{amount|number:2}}元</i></li>
			          </ul>
			          </div>
				      <div class="resultForm">
				      <div class="timeline">
			          <div class="timeline1 hang3"></div>
			          <div class="timemain">
			            	<div class="timeline_top" style="background:#f5f5f5"></div>
			                <div class="timeline_img"><img class="sjhengjian" src="images/c001.png"/></div>
			                <div class="timeline_top"></div>
