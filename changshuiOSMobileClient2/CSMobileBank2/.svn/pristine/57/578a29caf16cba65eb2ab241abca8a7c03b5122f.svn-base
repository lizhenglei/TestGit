</div>
<script>
feiyongCtrl.$inject = ["$scope","$remote","$targets","$context","$timeout"];
function feiyongCtrl($scope,$remote,$targets,$context,$timeout){
	$scope.init=function(){
		$scope.lis=$context.getJson().da;
	}
}
</script>