<div v-view-setup="feiyongCtrl" v-init="init()">
	<form name="transInput" class="bg_box" style="  background:none">
	<div class="box_cont" style="  background:none">
			<div style="  margin-top: 5px;">
				<div style="float: left;width: 24%;text-align:center;border: 1px solid #ccc;">费用月份</div>
				<div style="float: left;width: 24%;text-align:center;border: 1px solid #ccc;">账目类型</div>
				<div style="float: left;width: 24%;text-align:center;border: 1px solid #ccc;">欠费金额</div>
				<div style="float: left;width: 24%;text-align:center;border: 1px solid #ccc;">滞纳金</div>
			</div>
			<div style="  margin-top: 5px;" v-repeat="li in lis">
				<div style="float: left;width: 24%;line-height: 30px;height:30px;background-color: white;text-align:center;border: 1px solid #ccc;">{{li.month}}</div>
				<div style="float: left;width: 24%;line-height: 30px;height:30px;background-color: white;text-align:center;border: 1px solid #ccc;">{{li.name}}</div>
				<div style="float: left;width: 24%;line-height: 30px;height:30px;background-color: white;text-align:center;border: 1px solid #ccc;">{{li.arrearage|number:2}}</div>
				<div style="float: left;width: 24%;line-height: 30px;height:30px;background-color: white;text-align:center;border: 1px solid #ccc;">{{li.overdueFine|number:2}}</div>
			</div>
		</div>
		</form>
